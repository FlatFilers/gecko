OPTIONS="$@" ts-node --swc --project $(dirname $0)/tsconfig.json -r tsconfig-paths/register -e "import { commit, printResolveSummary, resolve } from '@flatfile/gecko' ; import generate from './gecko.tsx' ; async function main() { commit(await resolve(generate())) } ; main().catch(e => console.error(e)).then(printResolveSummary)"
