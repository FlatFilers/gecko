{
  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "baseUrl": "./",
    "esModuleInterop": true,
    "jsx": "react",
    "jsxFactory": "geckoJSX",
    "jsxFragmentFactory": "geckoJSX",
    "noEmit": true,
    "paths": {
      "@flatfile/gecko": ["./src/index.ts"],
      "@flatfile/gecko/*": ["./src/*"]
    },
    "resolveJsonModule": true,
    "strictNullChecks": true
  },
  "exclude": ["examples/*/project"]
}
