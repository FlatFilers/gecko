{
  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "baseUrl": "./",
    "esModuleInterop": true,
    "jsx": "react",
    "jsxFactory": "geckoJSX",
    "jsxFragmentFactory": "geckoJSX",
    "noEmit": true,
    "lib": ["ES2015"],
    "target": "ES2022",
    "paths": {
      "@flatfile/gecko": ["./src/index.ts"],
      "@flatfile/gecko/*": ["./src/*"]
    },
    "strictNullChecks": true,
    "module": "ES2022"
  },
  "exclude": ["examples/*/project"]
}
